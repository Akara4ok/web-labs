{
    "hosting": {
        "public": "./Frontend/dist",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "rewrites": [
            {
                "source": "/",
                "destination": "/index.html"
            },
            {
                "source": "/api",
                "function": "api"
            }
        ]
    },
    "functions": {
        "source": "functions"
    }
}
